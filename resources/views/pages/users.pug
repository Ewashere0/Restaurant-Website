html
    head
        title Lord of the Restaurants
        style
            include ../../styles/styles.css
    body(class="background")
        include layout

        ul(id='link', class='MainDivs', style='font-size:20px; line-height:30px; position: absolute; top: 50%; left: 50%; transform: translate(-50%, -50%); text-align: center;')
            each user in users 
                unless user.privacy 
                    li 
                        a(href = '/users/' + user._id) #{user.username}
